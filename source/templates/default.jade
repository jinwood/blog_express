doctype html
html
  head
    meta(name='viewport',content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no')
    link(rel='stylesheet', href='/css/index.css')
    link(rel='stylesheet',href='http://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css')
    title Julian Inwood - Software Developer
  body
    .header.jumbotron
      h1.page-title Hello

    div.navbar-header
      button.navbar-toggle.collapsed(data-toggle='collapse', data-target='#navbar')
        i.fa.fa-bars
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
    div.collapse.navbar-collapse#navbar
      ul.nav.navbar-nav
        li: a(href='/') Home
        li: a(href='/blog') Blog
        li: a(href='/contact') Contact
    
    .main-content
      block content

    .footer
        p https://julianinwood.co.uk #{locals.date}
  script(src='https://code.jquery.com/jquery-3.1.1.min.js' type='text/javascript')
  script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js' type='text/javascript')